{
    "manifest_version": 3,
    "name": "Kipik - Keep Peek",
    "description": "Explorez en un clin d'œil la stack et les détails des sites web que vous visitez.",
    "version": "1.0",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "permissions": [
      "tabs",
      "scripting",
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content-script.js"]
      }
    ],
    "host_permissions": [
      "<all_urls>"
    ]
  }
  